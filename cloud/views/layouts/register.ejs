<form action="/user/handleRegister" method="POST" class="form-register">
<h2 class="form-signin-heading">注册用户</h2>
<input type="text" class="form-control" placeholder="用户名" name="username">
<br>
<input type="text" class="form-control" placeholder="电话" name="phone">
<br>
<input type="text" class="form-control" placeholder="邮件" name="email">
<br>
<input type="password" class="form-control" placeholder="你的密码" name="password">
<br>
<input type="password" class="form-control" placeholder="重复密码" name="confirmation">
<input type="submit" class="btn btn-lg btn-primary btn-block" value="Create Account"/>
<input type="hidden" name="_csrf" value="..."/>

</form>	